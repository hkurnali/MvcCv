@model  MvcCv.Models.Entity.TblDeneyimlerim

@{
    ViewBag.Title = "DeneyimGuncelle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<br />

<h3 class="container"> Deneyim Güncelleme Sayfası</h3>
<form method="post" class="form-control-feedback">
   @Html.HiddenFor(x=>x.ID)
    <label>Sektör</label>
    @Html.TextBoxFor(x => x.Baslik, new { @class = "form-control" })
    <br />
    <label>Kurum Adı</label>
    @Html.TextBoxFor(x => x.AltBaslik, new { @class = "form-control" })
    <br />
    <label>Ünvan</label>
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    <label>Tarih</label>
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>

</form>
